CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
UNLOAD DATA
FROM TABLE FIREFLY1 HEADER CONST 'SERENITY' SAMPLE 10
LIMIT 9
( MAL
, KAYLEE POSITION(*)
, JAYNE POSITION(7)
, INARA CHAR(8) CCSID 1208 TRUNCATE
, BOOK VARCHAR(80) STRIP BOTH ' '
, SIMON STRIP TRAILING X'00'
, RIVER VARGRAPHIC STRIP LEADING X'FF'
, X CONSTANT 'AEIOU'
) WHEN ( MAL = 'A'
AND KAYLEE NOT BETWEEN 'SIMON' AND 'MAL'
AND INARA NOT IN ('B', 'C', 'D')
AND BOOK LIKE 'E_F%;)'
AND SIMON IS NOT NULL)
CLONE
PUNCHDDN SYSPUNCH
UNLDDN SYSREC
EBCDIC
CCSID (37, 1140) 
NOSUBS 
NOPAD
SPANNED NO
FORMAT INTERNAL
DELIMITED
  COLDEL '|'
  CHARDEL '"'
  DECPT ':'
FLOAT S390
MAXERR 4
SHRLEVEL CHANGE ISOLATION CS
DECFLOAT_ROUNDMODE ROUND_CEILING
IMPLICIT_TZ '-06:00'
PARALLEL 7
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
UNLOAD DATA
FROM TABLE FIREFLY1 HEADER CONST 'SERENITY' SAMPLE (10)
LIMIT (9)
( MAL
, KAYLEE POSITION(*)
, JAYNE POSITION(7)
, INARA CHAR(8) CCSID 1208 TRUNCATE
, BOOK VARCHAR(80) STRIP BOTH ' '
, SIMON STRIP TRAILING X'00'
, RIVER VARGRAPHIC STRIP LEADING X'FF'
, X CONSTANT 'AEIOU'
) WHEN ( MAL = 'A'
AND KAYLEE NOT BETWEEN 'SIMON' AND 'MAL'
AND INARA NOT IN ('B', 'C', 'D')
AND BOOK LIKE 'E_F%;)'
AND SIMON IS NOT NULL)
CLONE
PUNCHDDN (SYSPUNCH)
UNLDDN (SYSREC)
EBCDIC
CCSID (37, 1140) 
NOSUBS 
NOPAD
SPANNED (NO)
FORMAT (INTERNAL)
DELIMITED
  COLDEL ('|')
  CHARDEL ('"')
  DECPT (':')
FLOAT (S390)
FLOAT (IEEE)
MAXERR (4)
SHRLEVEL (CHANGE) ISOLATION (CS)
SHRLEVEL (CHANGE) ISOLATION (UR) REGISTER(YES)
DECFLOAT_ROUNDMODE (ROUND_CEILING)
DECFLOAT_ROUNDMODE (ROUND_DOWN)
DECFLOAT_ROUNDMODE (ROUND_FLOOR)
DECFLOAT_ROUNDMODE (ROUND_HALF_DOWN)
DECFLOAT_ROUNDMODE (ROUND_HALF_EVEN)
DECFLOAT_ROUNDMODE (ROUND_HALF_UP)
DECFLOAT_ROUNDMODE (ROUND_UP)
IMPLICIT_TZ ('-06:00')
PARALLEL (7)
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
UNLOAD DATA
FROM TABLE FIREFLY1 HEADER CONST 'SERENITY' SAMPLE 10
LIMIT 9
( MAL GRAPHIC EXTERNAL (77) TRUNCATE
, KAYLEE SMALLINT
, JAYNE INTEGER EXTERNAL (8)
, INARA BIGINT
, BOOK BINARY (8) TRUNCATE
, SIMON VARBINARY
, RIVER BINARY VARYING
, X CONSTANT 'AEIOU'
) 
WHEN ( MAL = 'A'
AND KAYLEE NOT BETWEEN 'SIMON' AND 'MAL'
AND INARA NOT IN ('B', 'C', 'D')
AND BOOK LIKE 'E_F%;)'
AND SIMON IS NOT NULL)
FROM TABLE FIREFLY2 HEADER CONST X'01020304' SAMPLE 10
LIMIT 9
( MAL STRIP BOTH X'09'
, KAYLEE CHAR DBCLOBF CCSID 1200 BINARYXML
, JAYNE BLOB
, INARA DECIMAL PACKED
, BOOK DECIMAL ZONED
, SIMON DECIMAL EXTERNAL
, RIVER DECIMAL PACKED (3,0)
) 
WHEN ( MAL = 'A'
AND KAYLEE NOT BETWEEN 'SIMON' AND 'MAL'
AND INARA NOT IN ('B', 'C', 'D')
AND BOOK LIKE 'E_F%;)'
AND SIMON IS NOT NULL)
FROM TABLE FIREFLY3 HEADER CONST X'01020304' SAMPLE 10
LIMIT 9
( MAL FLOAT EXTERNAL (6)
, KAYLEE DOUBLE
, JAYNE REAL
, INARA DATE EXTERNAL (10)
, BOOK TIME EXTERNAL (8)
, SIMON TIMESTAMP WITH TIME ZONE EXTERNAL (32)
, RIVER ROWID
) 
WHEN ( MAL = 'A'
AND KAYLEE NOT BETWEEN 'SIMON' AND 'MAL'
AND INARA NOT IN ('B', 'C', 'D')
AND BOOK LIKE 'E_F%;)'
AND SIMON IS NOT NULL)
FROM TABLE FIREFLY4 HEADER CONST X'01020304' SAMPLE 10
LIMIT 9
( MAL BLOB (1024) TRUNCATE
, KAYLEE CLOB (512) TRUNCATE CCSID 1208
, JAYNE DBCLOB (256) TRUNCATE CCSID 1200
, INARA DECFLOAT (34)
, BOOK DECFLOAT EXTERNAL (20)
, SIMON VARCHAR CLOBF CCSID 1208 TEMPLATENAME BINARYXML
, RIVER XML BINARYXML
) 
WHEN ( MAL = 'A'
AND KAYLEE NOT BETWEEN 'SIMON' AND 'MAL'
AND INARA NOT IN ('B', 'C', 'D')
AND BOOK LIKE 'E_F%;)'
AND SIMON IS NOT NULL)
CLONE
PUNCHDDN SYSPUNCH
UNLDDN SYSREC
EBCDIC
CCSID (37, 1140) 
NOSUBS 
NOPAD
SPANNED NO
FORMAT INTERNAL
DELIMITED
  COLDEL '|'
  CHARDEL '"'
  DECPT ':'
FLOAT IEEE
MAXERR 4
SHRLEVEL CHANGE ISOLATION UR REGISTER YES
DECFLOAT_ROUNDMODE ROUND_DOWN
IMPLICIT_TZ '-06:00'
PARALLEL 7
    '
  , ''
  );

