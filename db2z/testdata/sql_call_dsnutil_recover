CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
RECOVER
  TABLESPACE GNOMON.NEITH
  INDEXSPACE GNOMON.TUBMAN
  INDEX GNOMON.HUNTER
  TOLOGPOINT X'000008675309''
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
RECOVER
  TABLESPACE GNOMON.NEITH
  TORBA X'B00B00155AD1''
  , ''
  );

