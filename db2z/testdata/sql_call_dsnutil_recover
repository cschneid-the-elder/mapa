    'DBB.STUFF'
  , 'NO'
  , '
RECOVER
  TABLESPACE GNOMON.NEITH
  RESTOREBEFORE(X'B00B00155AD1')'
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
RECOVER
  TABLESPACE GNOMON.NEITH DSNUM ALL FROM "GNOMON".NEITH DSNUM ALL
  INDEXSPACE GNOMON.TUBMAN DSNUM ALL FROM GNOMON."TUBMAN" DSNUM ALL
  INDEX GNOMON.HUNTER DSNUM ALL FROM "GNOMON"."HUNTER" DSNUM ALL
  TORBA X'0123456789ABCDEF''
  , ''
  );

