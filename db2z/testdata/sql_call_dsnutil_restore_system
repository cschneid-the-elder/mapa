CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'RESTORE SYSTEM LOGONLY SWITCH VCAT SYSVALUEDDN(SYSUT1)'
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
RESTORE SYSTEM ALTERNATE_CP(FIREFLY1)
RESTOREBEFORE X'F8675309'
FROMDUMP DUMPCLASS(DCLS) RSA('SERENITY') TAPEUNITS (4)
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
RESTORE SYSTEM ALTERNATE_CP(FIREFLY1)
RESTOREBEFORE (X'F8675309')
FROMDUMP DUMPCLASS(DCLS) RSA('SERENITY') TAPEUNITS (4)
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
RESTORE SYSTEM ALTERNATE_CP(FIREFLY2)
RESTOREBEFORE X'F8675309'
FLASHCOPY_PPRCP NO
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
RESTORE SYSTEM ALTERNATE_CP(FIREFLY3)
RESTOREBEFORE X'F8675309'
FLASHCOPY_PPRCP PMNO
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
RESTORE SYSTEM ALTERNATE_CP(FIREFLY4)
RESTOREBEFORE X'F8675309'
FLASHCOPY_PPRCP PMPREF
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
RESTORE SYSTEM ALTERNATE_CP(FIREFLY5)
RESTOREBEFORE X'F8675309'
