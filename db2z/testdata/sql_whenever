EXEC SQL WHENEVER NOT FOUND GOTO 0100-NOT-FOUND-1 END-EXEC
EXEC SQL WHENEVER NOT FOUND GOTO 0100-NOT-FOUND END-EXEC
EXEC SQL WHENEVER NOT FOUND GOTO :0100-NOT-FOUND END-EXEC
EXEC SQL WHENEVER SQLERROR GOTO 9999-ERROR-HANDLER END-EXEC
EXEC SQL WHENEVER SQLERROR GOTO :9999-ERROR-HANDLER END-EXEC
EXEC SQL WHENEVER SQLWARNING GOTO 9999-WARNING-HANDLER END-EXEC
EXEC SQL WHENEVER SQLWARNING GOTO :9999-WARNING-HANDLER END-EXEC

EXEC SQL WHENEVER NOT FOUND GO TO 0100-NOT-FOUND END-EXEC
EXEC SQL WHENEVER NOT FOUND GO TO :0100-NOT-FOUND END-EXEC
EXEC SQL WHENEVER SQLERROR GO TO 9999-ERROR-HANDLER END-EXEC
EXEC SQL WHENEVER SQLERROR GO TO :9999-ERROR-HANDLER END-EXEC
EXEC SQL WHENEVER SQLWARNING GO TO 9999-WARNING-HANDLER END-EXEC
EXEC SQL WHENEVER SQLWARNING GO TO :9999-WARNING-HANDLER END-EXEC

EXEC SQL WHENEVER NOT FOUND CONTINUE END-EXEC
EXEC SQL WHENEVER SQLERROR CONTINUE END-EXEC
EXEC SQL WHENEVER SQLWARNING CONTINUE END-EXEC

EXEC SQL WHENEVER SQLERROR GO TO 9999_SQLERROR END-EXEC

