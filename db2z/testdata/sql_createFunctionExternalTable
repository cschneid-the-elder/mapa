CREATE FUNCTION FILLORY1
  ( 
    DEANFOGG  INT AS LOCATOR
  , WICKER    DECIMAL
  , WAUGH     FLOAT
  , COLDWATER REAL
  , ADIYODI   TABLE LIKE TRAVELER AS LOCATOR
  )
  RETURNS GENERIC TABLE
  LANGUAGE PLI
;

CREATE FUNCTION FILLORY2
  ( 
    DEANFOGG  INT AS LOCATOR
  ,           DECIMAL
  , WAUGH     FLOAT
  ,           REAL
  , ADIYODI   TABLE LIKE TRAVELER AS LOCATOR
  )
  RETURNS TABLE
  (
    WICKER     INT
  , WAUGH      DECIMAL
  , ADIYODI    FLOAT
  , HANSON     REAL
  , QUINN      CHAR
  , COLDWATER  VARCHAR
  , OLOFF_DIAZ GRAPHIC
  , HOBERMAN   CLOB        AS LOCATOR
  , PICKWICK   BLOB
  , TODD       VARGRAPHIC
  )
  SPECIFIC BRAKEBILLS
  PARAMETER 
    CCSID ASCII
    CCSID EBCDIC
    CCSID UNICODE
    VARCHAR NULTERM
    VARCHAR STRUCTURE
  EXTERNAL NAME $8675309
  EXTERNAL
  LANGUAGE ASSEMBLE
  LANGUAGE C
  LANGUAGE COBOL
  LANGUAGE PLI
  PARAMETER STYLE SQL
  NOT DETERMINISTIC
  DETERMINISTIC
  FENCED
  RETURNS NULL ON NULL INPUT
  CALLED ON NULL INPUT
  READS SQL DATA
  CONTAINS SQL
  NO SQL
  EXTERNAL ACTION
  NO EXTERNAL ACTION
  NO PACKAGE PATH
  PACKAGE PATH YELLOW_BRICK_ROAD
  NO SCRATCHPAD
  SCRATCHPAD 100
  NO FINAL CALL
  FINAL CALL
  DISALLOW PARALLEL
  NO DBINFO
  DBINFO
  CARDINALITY 42
  NO COLLID
  COLLID THE_LIBRARY
  WLM ENVIRONMENT FILLORY
  WLM ENVIRONMENT ( FILLORY, * )
  ASUTIME NO LIMIT
  ASUTIME LIMIT 2718281828
  STAY RESIDENT NO
  STAY RESIDENT YES
  PROGRAM TYPE SUB
  PROGRAM TYPE MAIN
  SECURITY DB2
  SECURITY USER
  SECURITY DEFINER
  RUN OPTIONS 'RPTSTG(ON)'
  INHERIT SPECIAL REGISTERS
  DEFAULT SPECIAL REGISTERS
  STATIC DISPATCH
  STOP AFTER SYSTEM DEFAULT FAILURES
  STOP AFTER 42 FAILURES
  CONTINUE AFTER FAILURE
  NOT SECURED
  SECURED
;

CREATE FUNCTION FILLORY3 (
        SQL XML
)
RETURNS TABLE(
        DEAN_FOGG VARCHAR(8)
)
DETERMINISTIC
NO EXTERNAL ACTION
READS SQL DATA
BEGIN
RETURN SELECT
        SCHEMA
FROM XMLTABLE('//breakbills/faculty/members'
    PASSING BY REF SQL
    COLUMNS
        "DEAN_FOGG" VARCHAR(8) PATH 'text()' WITH DEFAULT 'HENRY'
)
END;
;

CREATE FUNCTION FILLORY4 (
        SQL XML
)
RETURNS TABLE(
        DEAN_FOGG VARCHAR(8)
)
DETERMINISTIC
NO EXTERNAL ACTION
READS SQL DATA
BEGIN ATOMIC
RETURN SELECT
        SCHEMA
FROM XMLTABLE('//breakbills/faculty/members'
    PASSING BY REF SQL
    COLUMNS
        "DEAN_FOGG" VARCHAR(8) PATH 'text()' WITH DEFAULT 'HENRY'
)
END;
;

