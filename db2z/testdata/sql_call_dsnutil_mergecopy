CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
MERGECOPY 
  TABLESPACE THEBELT.CERES
  COPYDDN (COPY1,COPY2)
  NEWCOPY  NO
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
MERGECOPY 
  TABLESPACE "THEBELT".CERES
  COPYDDN (,COPY2)
  NEWCOPY  NO
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
MERGECOPY 
  TABLESPACE THEBELT."CERES"
  RECOVERYDDN (COPY1,COPY2)
  NEWCOPY  NO
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
MERGECOPY 
  TABLESPACE "THEBELT"."CERES"
  RECOVERYDDN (COPY1)
  NEWCOPY  NO
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
  TEMPLATE T1 UNIT(SYSDA) SPACE CYL
              DSN(T1.&SN..T&TI..COPY&IC.&LOCREM.)
              LIMIT(5 MB,T5)
  TEMPLATE T5 UNIT(3Z0)
              DSN(T5.&SN..T&TI..COPY&IC.&LOCREM.)
  MERGECOPY TABLESPACE INNER.EARTH  DSNUM ALL NEWCOPY NO
          COPYDDN T1
  MERGECOPY TABLESPACE INNER.MARS   DSNUM ALL NEWCOPY NO
          COPYDDN(T1)
  MERGECOPY TABLESPACE INNER.LUNA   DSNUM ALL NEWCOPY NO
          COPYDDN(T1)
  MERGECOPY TABLESPACE INNER.PHOBOS DSNUM ALL NEWCOPY NO
          COPYDDN(T1)
  MERGECOPY TABLESPACE INNER.DIEMOS DSNUM ALL NEWCOPY NO
          COPYDDN(T1)
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
MERGECOPY 
TABLESPACE INNER.LUNA 
DSNUM(ALL)
CLONE 
NEWCOPY YES 
NEWCOPY(YES) 
COPYDDN(COPYTB1)
WORKDDN WORKDDN
WORKDDN(WORKDDN)
    '
  , ''
  );


