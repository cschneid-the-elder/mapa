CALL SYSPROC.DSNUTILV(
  'DBB.STUFF'
  , 'NO'
  , '
   TEMPLATE C2C1_T1
            DSN(PICKWICK.C2C1.LB.&SN.)
            DISP(NEW,CATLG,CATLG)
            UNIT(SYSDA)

   TEMPLATE C2C1_T2
            DSN(PICKWICK.C2C1.RP.&SN.)
            DISP(NEW,CATLG,CATLG)
            UNIT(SYSDA)

   TEMPLATE C2C1_T3
            DSN(PICKWICK.C2C1.RB.&SN.)
            DISP(NEW,CATLG,CATLG)
            UNIT(SYSDA)

   COPYTOCOPY TABLESPACE TICK.PICKWICK
              FROMCOPY PICKWICK.COPY1.STEP1.TP01
              FROMVOLUME SCR03
              COPYDDN(,C2C1_T1)
              RECOVERYDDN(C2C1_T2,C2C1_T3)
    '
  , ''
  );

CALL SYSPROC.DSNUTILV(
  'DBB.STUFF'
  , 'NO'
  , '
COPYTOCOPY
      TABLESPACE MARTIN.CHATWIN DSNUM 2
          FROMLASTFULLCOPY COPYDDN(,COPY2)
      TABLESPACE MARTIN.CHATWIN DSNUM 3
          FROMLASTCOPY COPYDDN(,COPY3)
      TABLESPACE MARTIN.CHATWIN DSNUM 4
          FROMLASTINCRCOPY COPYDDN(,COPY4)
    '
  , ''
  );

CALL SYSPROC.DSNUTILV(
  'DBB.STUFF'
  , 'NO'
  , '
OPTIONS PREVIEW
LISTDEF CPY1
        INCLUDE TABLESPACES TABLESPACE BREAK*.F*GG*
        INCLUDE INDEXSPACES COPY YES INDEXSPACE BREAK*.I?FOGG*
OPTIONS OFF
TEMPLATE T4 UNIT(3Z0) DSN(T4.&SN..T&TI..COPY&IC.&LOCREM.)
TEMPLATE T3 UNIT(SYSDA) 
            SPACE CYL DSN(T3.&SN..T&TI..COPY&IC.&LOCREM.)
            LIMIT(5 MB,T4)
COPYTOCOPY LIST CPY1 COPYDDN(T3,T3)
    '
  , ''
  );

