     INDEX ALL FREQVAL NUMCOLS 1 COUNT 10 
     REPORT YES 
     UPDATE NONE
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REORG TABLESPACE AMOS.BURTON PART (11)
          NOSYSREC
          REPORTONLY 
          SHRLEVEL CHANGE MAPPINGTABLE BALTIMORE.TIMOTHY
          COPYDDN (SYSCOPY)
          OFFPOSLIMIT 11 INDREFLIMIT 15
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REORG TABLESPACE OPA.CERES
      SHRLEVEL CHANGE MAPPINGTABLE EROS
      COPYDDN(SYSCOPY1)
      OFFPOSLIMIT 9 INDREFLIMIT 9
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
   REORG TABLESPACE LIST KAMAL
       DRAIN_WAIT 30
       RETRY 4
       RETRY_DELAY 10
          STATISTICS
          TABLE (ALL) SAMPLE 60
          INDEX (ALL) FREQVAL NUMCOLS 2 COUNT 15
          SHRLEVEL CHANGE MAPPINGTABLE MAP5702
          LONGLOG DRAIN MAXRO DEFER DELAY 30
          COPYDDN (CPYTMP)
          SORTDEVT SYSDA SORTNUM 8
          PUNCHDDN SPUNCH
          DISCARDDN SDISC
          UNLDDN SREC
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REORG TABLESPACE
  NAOMI.NAGATA
  DISCARD
  FROM TABLE JAMES.HOLDEN
  WHEN (COL1 = 'ABC')
  SHRLEVEL NONE COPYDDN SYSCOPY
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REORG TABLESPACE BOBBIE.DRAPER SHRLEVEL REFERENCE
  PUNCHDDN LOADSTMT
  DISCARDDN SYSDISC
  UNLOAD CONTINUE
  DISCARD
    FROM TABLE MARS
      WHEN (PHOBOS <= 777 AND
            DIEMOS = X'0A0D')
    FROM TABLE LUNA
      WHEN (C1 = X'07FE' OR
            C2 = X'01030507')
    '
  , ''
  );

