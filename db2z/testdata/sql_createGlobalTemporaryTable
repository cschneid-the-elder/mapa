CREATE GLOBAL TEMPORARY TABLE MOYA (
    DARGO CHAR (8)
   ,ZHAAN VARCHAR (255)
   ,RYGEL TIMESTAMP
   ,PILOT BIGINT
   );

CREATE GLOBAL TEMPORARY TABLE MOYA (
    DARGO CHAR (8) NOT NULL
   ,ZHAAN VARCHAR (255)
   ,RYGEL TIMESTAMP NOT NULL
   ,PILOT BIGINT
   );

CREATE GLOBAL TEMPORARY TABLE MOYA (
    DARGO CHAR (8)
   ,ZHAAN VARCHAR (255)
   ,RYGEL TIMESTAMP
   ,PILOT BIGINT
   ) CCSID ASCII;

CREATE GLOBAL TEMPORARY TABLE MOYA (
    DARGO CHAR (8)
   ,ZHAAN VARCHAR (255)
   ,RYGEL TIMESTAMP
   ,PILOT BIGINT
   ) CCSID EBCDIC;

CREATE GLOBAL TEMPORARY TABLE MOYA (
    DARGO CHAR (8)
   ,ZHAAN VARCHAR (255)
   ,RYGEL TIMESTAMP
   ,PILOT BIGINT
   ) CCSID UNICODE;

CREATE GLOBAL TEMPORARY TABLE MOYA (
    DARGO CHAR VARYING (108)
   ,ZHAAN VARCHAR (255) NOT NULL
   ,RYGEL TIMESTAMP
   ,PILOT BIGINT NOT NULL
   ) CCSID ASCII;

CREATE GLOBAL TEMPORARY TABLE MOYA (
    DARGO    CHAR (8)
   ,ZHAAN    VARCHAR (255)
   ,RYGEL    TIMESTAMP
   ,AERYN    DOUBLE PRECISION
   ,CRICHTON REAL
   ,PILOT    BIGINT
   ,CHIANA   CHARACTER VARYING (1024) FOR BIT DATA
   ,JOOL     GRAPHIC (128) CCSID 1200
   ,NORANTI  BINARY VARYING (2048)
   ,SIKOZU   TIME
   ,SCORPIUS TIMESTAMP WITHOUT TIME ZONE
   ,HARVEY   TIMESTAMP WITHOUT TIMEZONE
   ) CCSID UNICODE;

CREATE GLOBAL TEMPORARY TABLE TALYN LIKE MOYA;

CREATE GLOBAL TEMPORARY TABLE TALYN LIKE MOYA CCSID ASCII;

