CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , '
TEMPLATE SYSCOPY1 DSN &DB..&TS..COPY&IC.&LR.&PB..D&DATE..T&TIME.
         UNIT(SYSDA) DISP (MOD,CATLG,CATLG)                              
LISTDEF COPYLIST 
        INCLUDE TABLESPACE ELIOT.FEN           
        INCLUDE TABLESPACE ELIOT.MARGO           
COPY LIST COPYLIST                                             
     COPYDDN (SYSCOPY1)
     CONCURRENT
    '
  , ''
  );

CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , '
LISTDEF TSLIST
        INCLUDE TABLESPACE TS1
        INCLUDE TABLESPACE TS2
        INCLUDE TABLESPACE TS3
TEMPLATE SYSCOPY DSN &DB..&TS..COPY&IC.&LR.&PB..D&DATE..T&TIME.
         UNIT(SYSDA) DISP (MOD,CATLG,CATLG)
TEMPLATE FILTR DSN FILTER.TEST1.&SN..D&DATE.
         UNIT(SYSDA) DISP (MOD,CATLG,DELETE)
COPY LIST TSLIST
          FILTERDDN(FILTR)
          COPYDDN(SYSCOPY)
          CONCURRENT
          SHRLEVEL REFERENCE
    '
  , ''
  );

