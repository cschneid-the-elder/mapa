CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , '
LISTDEF BREAKBILLS INCLUDE TABLESPACES TABLESPACE STUDENTS.* 
TEMPLATE HOME DSN('&DB..&COPY..HOME(+1)') UNIT CART STACK YES 
TEMPLATE AWAY DSN('&DB..&COPY..AWAY(+1)') UNIT CART STACK YES 
COPY LIST BREAKBILLS PARALLEL(10) TAPEUNITS(8)
 COPYDDN(HOME) RECOVERYDDN(AWAY)
    '
  , ''
  );

