CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , '
COPY
  TABLESPACE JULIA.WICKER
             COPYDDN (COPY1,COPY2)
             RECOVERYDDN (COPY3,COPY4)
  INDEX MARGO.HANSON1
        COPYDDN (COPY5,COPY6)
        RECOVERYDDN (COPY7,COPY8)
  INDEX MARGO.HANSON2
        COPYDDN (COPY9,COPY10)
        RECOVERYDDN (COPY11,COPY12)
  INDEX MARGO.HANSON3
        COPYDDN (COPY13,COPY14)
        RECOVERYDDN (COPY15,COPY16)
  TABLESPACE JULIA.WICKER
             COPYDDN (COPY17,COPY18)
             RECOVERYDDN (COPY19,COPY20)
  INDEX MARGO.XHANSON1
        COPYDDN (COPY21,COPY22)
        RECOVERYDDN (COPY23,COPY24)
  INDEX MARGO.XHANSON2
        COPYDDN (COPY25,COPY26)
        RECOVERYDDN (COPY27,COPY28)
 PARALLEL(4)
 SHRLEVEL REFERENCE
    '
  , ''
  );

CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , '
TEMPLATE T2 UNIT(SYSDA) SPACE CYL           
            DSN(T2.&SN..T&TI..COPY&IC.&LOCREM.)  
TEMPLATE T1 UNIT(SYSDA) SPACE CYL           
            DSN(T1.&SN..T&TI..COPY&IC.&LOCREM.)
            LIMIT(5 MB,T2)                           
LISTDEF COPYLIST                                  
        INCLUDE TABLESPACE ELIOT.WAUGH       
        INCLUDE INDEX MARGO.HANSON1               
        INCLUDE INDEX MARGO.HANSON2               
        INCLUDE INDEX MARGO.HANSON3               
        INCLUDE TABLESPACE ALICE.QUINN       
        INCLUDE INDEX QUENTIN.XCOLD1                
        INCLUDE INDEX QUENTIN.XCOLD2                
COPY LIST COPYLIST COPYDDN(T1,T1)            
     RECOVERYDDN(T1,T1)                     
     PARALLEL(4) SHRLEVEL REFERENCE
    '
  , ''
  );

CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , '
TEMPLATE LARGE DSN &DB..&TS..D&DA..T&TI. UNIT TAPE
TEMPLATE SMALL DSN &DB..&TS..D&DA..T&TI. UNIT SYSALLDA LIMIT( 20 CYL, LARGE )
COPY TABLESPACE SMALL.TS COPYDDN( SMALL )
COPY TABLESPACE LARGE.TS COPYDDN( SMALL )
    '
  , ''
  );

CALL SYSPROC.DSNUTILS(
  'DBB.STUFF'
  , 'NO'
  , '
TEMPLATE A1 DSN(&DB..&SP..COPY1) UNIT CART STACK YES
