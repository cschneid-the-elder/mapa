CREATE FUNCTION BRAKEBILLS
  (
    DEANFOGG    INT AS LOCATOR
  , Z1          DECIMAL
  , WAUGH       FLOAT
  , Z2          REAL
  , ADIYODI     TABLE LIKE TRAVELER AS LOCATOR
  , WICKER      CHAR
  , HANSON      CHARACTER
  , QUINN       VARCHAR
  , ORLOFF_DIAZ VARGRAPHIC
  , COLDWATER   BINARY
  , FEN         VARBINARY
  , HOBERMAN    BINARY LARGE OBJECT (4M)
  , SCHIFF      BINARY VARYING (33)
  , LIPSON      TIMESTAMP
  , TODD        TIMESTAMP (6)
  , CHATWIN     TIMESTAMP WITHOUT TIME ZONE
  )
  WRAPPED loremIpsumDolorSitAmetConsecteturAdipiscingElit
@

CREATE FUNCTION SCHOLOMANSE ()
  RETURNS CHAR(80) FOR MIXED DATA
  DETERMINISTIC
  BEGIN
    RETURN 'SPACES';
  END @

CREATE FUNCTION SCHOLOMANSE ()
  RETURNS CHAR(80) FOR MIXED DATA CCSID
  DETERMINISTIC
  BEGIN
    RETURN 'SPACES';
  END @

CREATE FUNCTION SCHOLOMANSE ()
  RETURNS CHAR(80) FOR MIXED DATA CCSID ASCII
  DETERMINISTIC
  BEGIN
    RETURN 'SPACES';
  END @

CREATE FUNCTION SCHOLOMANSE ()
  RETURNS CHAR(80) CCSID FOR MIXED DATA
  DETERMINISTIC
  BEGIN
    RETURN 'SPACES';
  END @

CREATE FUNCTION SCHOLOMANSE ()
  RETURNS CHAR(80) CCSID UNICODE FOR MIXED DATA
  DETERMINISTIC
  BEGIN
    RETURN 'SPACES';
  END @

CREATE FUNCTION BRAKEBILLS
  (
    DEANFOGG    INT AS LOCATOR
  , Z1          DECIMAL
  , WAUGH       FLOAT
  , Z2          REAL
  , ADIYODI     TABLE LIKE TRAVELER AS LOCATOR
  , WICKER      CHAR
  , HANSON      CHARACTER
  , QUINN       VARCHAR
  , ORLOFF_DIAZ VARGRAPHIC
  , COLDWATER   BINARY
  , FEN         VARBINARY
  , HOBERMAN    BINARY LARGE OBJECT (4M)
  , SCHIFF      BINARY VARYING (33)
  , LIPSON      TIMESTAMP
  , TODD        TIMESTAMP (6)
  , CHATWIN     TIMESTAMP WITHOUT TIME ZONE
  )
  RETURNS DATE
  LANGUAGE SQL
  SPECIFIC BRAKEBILLS
  PARAMETER CCSID ASCII
  PARAMETER CCSID EBCDIC
  PARAMETER CCSID UNICODE
  NOT DETERMINISTIC
  DETERMINISTIC
  EXTERNAL ACTION
  NO EXTERNAL ACTION
  READS SQL DATA
  CONTAINS SQL
  STATIC DISPATCH
  CALLED ON NULL INPUT
  NOT SECURED
  SECURED
  BEGIN
    DECLARE JULIA     CHAR;
    DECLARE QUENTIN   CHARACTER;
    DECLARE MARGO     CHARACTER VARYING (33);
    DECLARE ELLIOT    VARCHAR (42);
    DECLARE PENNY     INT;
    DECLARE KADY      INTEGER;
    DECLARE ALICE     DOUBLE PRECISION;
    
    DECLARE EXIT HANDLER FOR SQLEXCEPTION
    BEGIN
      GET DIAGNOSTICS CONDITION 1
          SAVE_ERRMSG   = MESSAGE_TEXT
        , SAVE_SQLCODE  = DB2_RETURNED_SQLCODE
        , SAVE_SQLSTATE = RETURNED_SQLSTATE;
      SIGNAL SQLSTATE '123456'
        SET MESSAGE_TEXT = 'This is bad';
    END;
    
    ACTUAL_WORK: BEGIN
      SELECT C1 INTO MY_C1 FROM T1 WHERE C2 = ALICE;
      CASE
        WHEN SQLCODE = 100 THEN
          UPDATE T2 SET C1 = KADY WHERE C2 = ALICE;
        WHEN MY_C1 > 42 THEN
          MY_LOOP: LOOP
            INSERT INTO T3 (C1, C2) VALUES (CURRENT TIMESTAMP, CURRENT DATE);
            SET MY_COUNT = MY_COUNT + 1;
            IF MY_COUNT = MY_C1 THEN LEAVE MY_LOOP; END IF;
          END LOOP MY_LOOP;
        ELSE
          SIGNAL SQLSTATE '314159' ('I am confused');
      END CASE;
    END ACTUAL_WORK;
  END;

@

CREATE FUNCTION SCHOLOMANSE ()
  RETURNS CHAR(80) CCSID FOR MIXED DATA
  DETERMINISTIC
  BEGIN
    SET CURRENT TEMPORAL SYSTEM_TIME = CURRENT TIMESTAMP;
    RETURN 'SPACES';
  END @


