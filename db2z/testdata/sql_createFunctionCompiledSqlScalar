CREATE FUNCTION SCHOLOMANSE
  ()
  RETURNS CHAR(80) FOR SBCS DATA CCSID
  VERSION 8675309
  NOT DETERMINISTIC
  BEGIN
    RETURN 'SPACES';
  END @

CREATE FUNCTION SCHOLOMANSE
  ()
  RETURNS CHAR(80) FOR SBCS DATA CCSID UNICODE
  VERSION 8675309
  NOT DETERMINISTIC
  BEGIN
    RETURN 'SPACES';
  END @

CREATE FUNCTION SCHOLOMANSE
  ()
  RETURNS CHAR(80) CCSID
  VERSION 8675309
  NOT DETERMINISTIC
  BEGIN
    RETURN 'SPACES';
  END @

CREATE FUNCTION SCHOLOMANSE
  ()
  RETURNS CHAR(80) CCSID UNICODE
  VERSION 8675309
  NOT DETERMINISTIC
  BEGIN
    RETURN 'SPACES';
  END @

CREATE FUNCTION SCHOLOMANSE
  ()
  RETURNS CHAR(80) CCSID FOR SBCS DATA
  VERSION 8675309
  NOT DETERMINISTIC
  BEGIN
    RETURN 'SPACES';
  END @

CREATE FUNCTION BRAKEBILLS
  (
    DEANFOGG    INT
  , ADIYODI     DECIMAL
  , WAUGH       FLOAT
  , MARGO       REAL
  , WICKER      CHAR
  , HANSON      CHARACTER
  , QUINN       VARCHAR (1)
  , ORLOFF_DIAZ VARGRAPHIC (1)
  , COLDWATER   BINARY
  , FEN         VARBINARY (1)
  , HOBERMAN    BINARY LARGE OBJECT (4M)
  , SCHIFF      BINARY VARYING (33)
  , LIPSON      TIMESTAMP
  , TODD        TIMESTAMP (6)
  , CHATWIN     TIMESTAMP WITHOUT TIME ZONE
  , PLOVER      TIMESTAMP WITHOUT TIMEZONE
  )
  RETURNS XML
  VERSION V1
  LANGUAGE SQL
  SPECIFIC FILLORY
  NOT DETERMINISTIC
  DETERMINISTIC
  EXTERNAL ACTION
  NO EXTERNAL ACTION
  READS SQL DATA
  CONTAINS SQL
  MODIFIES SQL DATA
  CALLED ON NULL INPUT
  RETURNS NULL ON NULL INPUT
  STATIC DISPATCH
  ALLOW PARALLEL
  DISALLOW PARALLEL
  DISALLOW DEBUG MODE
  ALLOW DEBUG MODE
  DISABLE DEBUG MODE
  PARAMETER CCSID ASCII
  PARAMETER CCSID EBCDIC
  PARAMETER CCSID UNICODE
  QUALIFIER FOGG
  PACKAGE OWNER THE_LIBRARY
  PACKAGE OWNER THE_LIBRARY AS ROLE
  PACKAGE OWNER THE_LIBRARY AS USER
  ASUTIME NO LIMIT
  ASUTIME LIMIT 42
  INHERIT SPECIAL REGISTERS
  DEFAULT SPECIAL REGISTERS
  WLM ENVIRONMENT FOR DEBUG MODE EARTH
  CURRENT DATA NO
  CURRENT DATA YES
  DEGREE 1
  DEGREE ANY
  CONCURRENT ACCESS RESOLUTION USE CURRENTLY COMMITTED
  CONCURRENT ACCESS RESOLUTION WAIT FOR OUTCOME
  DYNAMICRULES RUN
  DYNAMICRULES BIND
  DYNAMICRULES DEFINEBIND
  DYNAMICRULES DEFINERUN
  DYNAMICRULES INVOKEBIND
  DYNAMICRULES INVOKERUN
  APPLICATION ENCODING SCHEME ASCII
  APPLICATION ENCODING SCHEME EBCDIC
  APPLICATION ENCODING SCHEME UNICODE
  WITHOUT EXPLAIN
  WITH EXPLAIN
  WITHOUT IMMEDIATE WRITE
  WITH IMMEDIATE WRITE
  ISOLATION LEVEL CS
  ISOLATION LEVEL RS
  ISOLATION LEVEL RR
  ISOLATION LEVEL UR
  OPTHINT ''
  OPTHINT 'NO'
  QUERY ACCELERATION NONE
  QUERY ACCELERATION ENABLE
  QUERY ACCELERATION ENABLE WITH FAILBACK
  QUERY ACCELERATION ELIGIBLE
  QUERY ACCELERATION ALL
  GET_ACCEL_ARCHIVE NO
  GET_ACCEL_ARCHIVE YES
  ACCELERATION WAITFORDATA 1234.5
  ACCELERATOR MARINA
  SQL PATH HERE, SYSTEM PATH, SESSION USER, USER
  REOPT NONE
  REOPT ALWAYS
  REOPT ONCE
  VALIDATE RUN
  VALIDATE BIND
  ROUNDING DEC_ROUND_CEILING
  ROUNDING DEC_ROUND_DOWN
  ROUNDING DEC_ROUND_FLOOR
  ROUNDING DEC_ROUND_HALF_DOWN
  ROUNDING DEC_ROUND_HALF_EVEN
  ROUNDING DEC_ROUND_HALF_UP
  ROUNDING DEC_ROUND_UP
  DATE FORMAT ISO
  DATE FORMAT EUR
  DATE FORMAT USA
  DATE FORMAT JIS
  DATE FORMAT LOCAL
  DECIMAL(15)
  DECIMAL (31)
  DECIMAL(15,1)
  DECIMAL (31,0)
  FOR UPDATE CLAUSE REQUIRED
  FOR UPDATE CLAUSE OPTIONAL
  TIME FORMAT ISO
  TIME FORMAT EUR
  TIME FORMAT USA
  TIME FORMAT JIS
  TIME FORMAT LOCAL
  SECURED
  NOT SECURED
  BUSINESS_TIME SENSITIVE YES
  BUSINESS_TIME SENSITIVE NO
  SYSTEM_TIME SENSITIVE YES
  SYSTEM_TIME SENSITIVE NO
  ARCHIVE SENSITIVE YES
  ARCHIVE SENSITIVE NO
  APPLCOMPAT V12R00
  APPLCOMPAT V12R00M500
  CONCENTRATE STATEMENTS OFF
  CONCENTRATE STATEMENTS WITH LITERALS
  BEGIN
    DECLARE JULIA     CHAR;
    DECLARE QUENTIN   CHARACTER;
    DECLARE MARGO     CHARACTER VARYING (33);
    DECLARE ELLIOT    VARCHAR (42);
    DECLARE PENNY     INT;
    DECLARE KADY      INTEGER;
    DECLARE ALICE     DOUBLE PRECISION;
    
    DECLARE EXIT HANDLER FOR SQLEXCEPTION
    BEGIN
      GET DIAGNOSTICS CONDITION 1
          SAVE_ERRMSG   = MESSAGE_TEXT
        , SAVE_SQLCODE  = DB2_RETURNED_SQLCODE
        , SAVE_SQLSTATE = RETURNED_SQLSTATE;
      SIGNAL SQLSTATE '123456'
        SET MESSAGE_TEXT = 'This is bad';
    END;
    
    ACTUAL_WORK: BEGIN
      UPDATE T1 SET
          C1 = DEANFOGG
        , C2 = MARGO
        , C3 = ADIYODI
      WHERE DATE(LIPSON) = CURRENT DATE;
      IF HANSON <> WICKER THEN
        UPDATE T2 SET
            C1 = QUINN
        WHERE DATE(LIPSON) = CURRENT DATE;
      END IF;
    END ACTUAL_WORK;
  END;

@

CREATE FUNCTION BRAKEBILLS
  (
    DEANFOGG    INT AS LOCATOR
  )
  RETURNS DATE
  LANGUAGE SQL
  SPECIFIC BRAKEBILLS
  PARAMETER CCSID ASCII
  PARAMETER CCSID EBCDIC
  PARAMETER CCSID UNICODE
  BEGIN
    DECLARE JULIA     CHAR;
    DECLARE W_S_Q_L_I_D         CHAR(128) CCSID EBCDIC FOR SBCS DATA;
    DECLARE W_SQLTEXT   CHAR(100) CCSID EBCDIC FOR SBCS DATA DEFAULT '1' ;

    DECLARE EXIT HANDLER FOR SQLEXCEPTION
    BEGIN
      GET DIAGNOSTICS CONDITION 1
          SAVE_ERRMSG   = MESSAGE_TEXT
        , SAVE_SQLCODE  = DB2_RETURNED_SQLCODE
        , SAVE_SQLSTATE = RETURNED_SQLSTATE;
      SIGNAL SQLSTATE '123456'
        SET MESSAGE_TEXT = 'This is bad';
    END;
  END;

@

