CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REBUILD INDEX (
  GNOMON.NEITH
  , "GNOMON".TUBMAN PART 1
  , GNOMON."HUNTER")
  SHRLEVEL CHANGE 
    MAXRO DEFER 
    LONGLOG TERM 
    DELAY 42
  DRAIN_WAIT 7 
  RETRY 11 
  RETRY_DELAY 13
  CLONE
  SCOPE ALL
  REUSE
  SORTDEVT WRK
  SORTNUM 17
  STATISTICS 
    REPORT YES 
    KEYCARD
    FREQVAL 
      NUMCOLS 3 
    FREQVAL 
      NUMCOLS 5
      COUNT 7 MOST 
    HISTOGRAM
      NUMCOLS 9
      NUMQUANTILES 975
    UPDATE ACCESSPATH
    INVALIDATECACHE YES
    HISTORY SPACE
    FORCEROLLUP YES
  FLASHCOPY 
    CONSISTENT 
    FCCOPYDDN(REBUILD)
  PARALLEL(7)
  RBALRSN_CONVERSION NONE'
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REBUILD INDEX (
  GNOMON.NEITH
  , "GNOMON".TUBMAN PART(1)
  , GNOMON."HUNTER")
  SHRLEVEL(REFERENCE) 
  SHRLEVEL(CHANGE) 
    MAXRO(DEFER)
    LONGLOG(TERM)
    DELAY(42)
  DRAIN_WAIT(7) 
  RETRY(11) 
  RETRY_DELAY(13)
  CLONE
  SCOPE(ALL)
  SCOPE(PENDING)
  REUSE
  SORTDEVT(WRK)
  SORTNUM(17)
  STATISTICS 
    REPORT(YES)
    KEYCARD
    FREQVAL 
      NUMCOLS(3)
    FREQVAL 
      NUMCOLS(5)
      COUNT(7) MOST 
    HISTOGRAM
      NUMCOLS(9)
      NUMQUANTILES(975)
    UPDATE(ACCESSPATH)
    INVALIDATECACHE(YES)
    HISTORY(SPACE)
    FORCEROLLUP(YES)
  FLASHCOPY 
    CONSISTENT 
    FCCOPYDDN(REBUILD)
  PARALLEL(7)
  RBALRSN_CONVERSION(NONE)'
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'REBUILD INDEX (ALL) TABLESPACE GNOMON.'HUNTER''
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REBUILD INDEX (GNOMON.NEITH PART 1)
  STATISTICS FORCEROLLUP YES
  SCOPE ALL
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'REBUILD INDEXSPACE (GNOMON.TUBMAN) CLONE'
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'REBUILD INDEXSPACE (ALL) TABLESPACE GNOMON.TUBMAN CLONE'
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , 'REBUILD INDEXSPACE LIST LIST CLONE'
  , ''
  );



