CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
LOCATE 
  TABLESPACE "FIREFLY".SERENITY PART 1 PAGE X'0001'
    VERIFY OFFSET X'1' DATA X'0101'
LOCATE
  TABLESPACE FIREFLY."SERENITY" KEY '2222' INDEX "I2"
    VERIFY OFFSET 2 DATA '2222'
LOCATE
  INDEXSPACE 'MAL' PART 3 PAGE 33
    REPLACE OFFSET X'3' DATA '3333'
LOCATE
  INDEX 'KAYLEE' PART 4 PAGE X'44'
    DELETE DATAONLY
    DUMP OFFSET X'4444' LENGTH X'44' PAGES X'4'
    DUMP PAGES *
    DUMP MAP 0
    DUMP DATA 7
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG (YES)
LOCATE 
  TABLESPACE "FIREFLY".SERENITY PART (1) PAGE (X'0001')
    VERIFY OFFSET (X'1') DATA (X'0101')
LOCATE
  TABLESPACE FIREFLY."SERENITY" KEY '2222' INDEX "I2"
    VERIFY OFFSET (2) DATA ('2222')
LOCATE
  INDEXSPACE 'MAL' PART (3) PAGE (33)
    REPLACE OFFSET X'3' DATA '3333'
LOCATE
  INDEX 'KAYLEE' PART (4) PAGE (X'44')
    DELETE (DATAONLY)
    DUMP OFFSET (X'4444') LENGTH (X'44') PAGES (X'4')
    DUMP PAGES (*)
    DUMP MAP (0)
    DUMP DATA (7)
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
LOCATE 
  TABLESPACE FIREFLY.SERENITY ROWID X'060504030201' VERSION X'0A0B0C0D0E0F'
    DELETE DATAONLY
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
LOCATE 
  TABLESPACE FIREFLY.SERENITY ROWID (X'060504030201') VERSION (X'0A0B0C0D0E0F')
    DELETE (DATAONLY)
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
LOCATE 
  TABLESPACE FIREFLY.SERENITY DOCID X'060504030201' DELETE DATAONLY
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
LOCATE 
  TABLESPACE FIREFLY.SERENITY DOCID (X'060504030201') DELETE (DATAONLY)
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
  SET TABLESPACE FIREFLY.SERENITY     PART 1 NOCOPYPEND
  SET TABLESPACE 'FIREFLY'.SERENITY   PART 2 NORCVRPEND
  SET TABLESPACE FIREFLY.'SERENITY'   PART 3 NOCHECKPEND
  SET TABLESPACE 'FIREFLY'.'SERENITY' PART 4 NOAUXWARN
  SET TABLESPACE "FIREFLY".SERENITY   PART 5 NOAUXCHKP
  SET TABLESPACE FIREFLY."SERENITY"   PART 6 NOAREORPENDSTAR
  SET TABLESPACE "FIREFLY"."SERENITY" PART 7 NOAREORPEND
  SET TABLESPACE FIREFLY.SERENITY     PART 8 PRO
  SET TABLESPACE FIREFLY.SERENITY     PART 9 NOPRO
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
  SET INDEX (FIREFLY.SERENITY     PART 1) NOCOPYPEND
  SET INDEX ('FIREFLY'.SERENITY   PART 2) NORCVRPEND
  SET INDEX (FIREFLY.'SERENITY'   PART 3) NORBDPEND
  SET INDEX ('FIREFLY'.'SERENITY' PART 4) NOCHECKPEND
  SET INDEX (FIREFLY."SERENITY"   PART 6) NOAREORPENDSTAR
  SET INDEX ("FIREFLY"."SERENITY" PART 7) NOAREORPEND
  SET INDEX (FIREFLY.SERENITY     PART 8) RBDPEND
  SET INDEX (FIREFLY.SERENITY     PART 9) PSRBDPEND
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
  SET INDEX (ALL) TABLESPACE FIREFLY.SERENITY      NOCOPYPEND
  SET INDEX (ALL) TABLESPACE 'FIREFLY'.SERENITY    NORCVRPEND
  SET INDEX (ALL) TABLESPACE FIREFLY.'SERENITY'    NORBDPEND
  SET INDEX (ALL) TABLESPACE 'FIREFLY'.'SERENITY'  NOCHECKPEND
  SET INDEX (ALL) TABLESPACE FIREFLY."SERENITY"    NOAREORPENDSTAR
  SET INDEX (ALL) TABLESPACE "FIREFLY"."SERENITY"  NOAREORPEND
  SET INDEX (ALL) TABLESPACE FIREFLY.SERENITY      RBDPEND
  SET INDEX (ALL) TABLESPACE FIREFLY.SERENITY      PSRBDPEND
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
  SET INDEXSPACE (FIREFLY.SERENITY     PART 1) NOCOPYPEND
  SET INDEXSPACE ('FIREFLY'.SERENITY   PART 2) NORCVRPEND
  SET INDEXSPACE (FIREFLY.'SERENITY'   PART 3) NORBDPEND
  SET INDEXSPACE ('FIREFLY'.'SERENITY' PART 4) NOCHECKPEND
  SET INDEXSPACE (FIREFLY."SERENITY"   PART 6) NOAREORPENDSTAR
  SET INDEXSPACE ("FIREFLY"."SERENITY" PART 7) NOAREORPEND
  SET INDEXSPACE (FIREFLY.SERENITY     PART 8) RBDPEND
  SET INDEXSPACE (FIREFLY.SERENITY     PART 9) PSRBDPEND
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
  SET INDEXSPACE (ALL) TABLESPACE FIREFLY.SERENITY      NOCOPYPEND
  SET INDEXSPACE (ALL) TABLESPACE 'FIREFLY'.SERENITY    NORCVRPEND
  SET INDEXSPACE (ALL) TABLESPACE FIREFLY.'SERENITY'    NORBDPEND
  SET INDEXSPACE (ALL) TABLESPACE 'FIREFLY'.'SERENITY'  NOCHECKPEND
  SET INDEXSPACE (ALL) TABLESPACE FIREFLY."SERENITY"    NOAREORPENDSTAR
  SET INDEXSPACE (ALL) TABLESPACE "FIREFLY"."SERENITY"  NOAREORPEND
  SET INDEXSPACE (ALL) TABLESPACE FIREFLY.SERENITY      RBDPEND
  SET INDEXSPACE (ALL) TABLESPACE FIREFLY.SERENITY      PSRBDPEND
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
  DBD DROP DATABASE FIREFLY DBID X'0001'
  DBD TEST DATABASE FIREFLY
  DBD DIAGNOSE DATABASE FIREFLY OUTDDN SYSUT1
  DBD REBUILD DATABASE FIREFLY OUTDDN SYSUT2
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
  DBD DROP DATABASE FIREFLY DBID (X'0001')
  DBD TEST DATABASE FIREFLY
  DBD DIAGNOSE DATABASE FIREFLY OUTDDN (SYSUT1)
  DBD REBUILD DATABASE FIREFLY OUTDDN (SYSUT2)
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
  LEVELID TABLESPACE "FIREFLY"."SERENITY"
  LEVELID INDEX "FIREFLY".SERENITY
  LEVELID INDEXSPACE FIREFLY."SERENITY"
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
  CATALOG TABLESPACE "FIREFLY"."SERENITY"
  CATALOG INDEX "FIREFLY".SERENITY TEST
  CATALOG INDEXSPACE FIREFLY."SERENITY"
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
  INSERTVERSIONPAGES SETCURRENTVERSION TABLESPACE FIREFLY.SERENITY SHRLEVEL CHANGE
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG (YES)
  INSERTVERSIONPAGES SETCURRENTVERSION TABLESPACE FIREFLY.SERENITY SHRLEVEL (CHANGE)
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
  WRITELOG TABLESPACE FIREFLY.SERENITY PART 2 TYPE 3 SUBTYPE 4 TEXT X'010203040506''
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
  WRITELOG TABLESPACE FIREFLY.SERENITY PART (2) TYPE (3) SUBTYPE (4) TEXT (X'010203040506')'
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG NO
LOCATE 
  TABLESPACE "FIREFLY".SERENITY PART 1 PAGE X'0001'
    VERIFY OFFSET X'1' DATA X'0101'
    REPLACE RESET
    REPLACE OFFSET X'0002' DATA X'0202'
    REPLACE DATA X'0003'
    REPLACE DATA 'ABCD'
    DELETE DATAONLY
    DELETE
    DUMP MAP
    DUMP DATA
    DUMP MAP 0
    DUMP DATA 0
    DUMP OFFSET 4
    DUMP OFFSET X'0005'
    DUMP LENGTH 6
    DUMP LENGTH X'0007'
    DUMP PAGES X'0008'
    DUMP PAGES 9
    DUMP PAGES *
SET
  TABLESPACE SERENITY
SET
  INDEX (ALL) TABLESPACE SERENITY
SET
  INDEXSPACE (ALL) TABLESPACE SERENITY
DBD
  DROP DATABASE ALLIANCE DBID X'ABCD'
DBD 
  TEST DATABASE ALLIANCE
DBD
  TEST DATABASE ALLIANCE OUTDDN SYSUT9
DBD
  DIAGNOSE DATABASE ALLIANCE
DBD
  REBUILD DATABASE ALLIANCE
LEVELID
  TABLESPACE INARA PART 1
LEVELID
  INDEX MAL
LEVELID
  INDEXSPACE WASH.ZOE PART 3
CATALOG
  TABLESPACE JAYNE.COBB TEST
CATALOG
  INDEX KAYLEE.FRYE
CATALOG
  INDEXSPACE RIVER.TAM TEST
INSERTVERSIONPAGES
  SETCURRENTVERSION TABLESPACE SIMON.TAM SHRLEVEL CHANGE
INSERTVERSIONPAGES
  TABLESPACE SAFFRON
    '
  , ''
  );

CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG NO
LOCATE 
  TABLESPACE "FIREFLY".SERENITY PART (1) PAGE (X'0001')
    VERIFY OFFSET (X'1') DATA (X'0101')
    REPLACE RESET
    REPLACE OFFSET (X'0002') DATA (X'0202')
    REPLACE DATA (X'0003')
    REPLACE DATA ('ABCD')
    DELETE (DATAONLY)
    DELETE
    DUMP MAP
    DUMP DATA
    DUMP MAP(0)
    DUMP DATA(0)
    DUMP OFFSET (4)
    DUMP OFFSET (X'0005')
    DUMP LENGTH (6)
    DUMP LENGTH (X'0007')
    DUMP PAGES (X'0008')
    DUMP PAGES (9)
    DUMP PAGES (*)
SET
  TABLESPACE SERENITY
SET
  INDEX (ALL) TABLESPACE SERENITY
SET
  INDEXSPACE (ALL) TABLESPACE SERENITY
DBD
  DROP DATABASE ALLIANCE DBID (X'ABCD')
DBD 
  TEST DATABASE ALLIANCE
DBD
  TEST DATABASE ALLIANCE OUTDDN (SYSUT9)
DBD
  DIAGNOSE DATABASE ALLIANCE
DBD
  REBUILD DATABASE ALLIANCE
LEVELID
  TABLESPACE INARA PART (1)
LEVELID
  INDEX MAL
LEVELID
  INDEXSPACE WASH.ZOE PART (3)
CATALOG
  TABLESPACE JAYNE.COBB TEST
CATALOG
  INDEX KAYLEE.FRYE
CATALOG
  INDEXSPACE RIVER.TAM TEST
INSERTVERSIONPAGES
  SETCURRENTVERSION TABLESPACE SIMON.TAM SHRLEVEL (CHANGE)
INSERTVERSIONPAGES
  TABLESPACE SAFFRON
    '
  , ''
  );


CALL SYSPROC.DSNUTILU (
    'DBB.STUFF'
  , 'NO'
  , '
REPAIR OBJECT LOG YES
LOCATE
  TABLESPACE FIREFLY."SERENITY" RID X'0D0A'
    VERIFY OFFSET 1 DATA '1111'
LOCATE
  TABLESPACE FIREFLY."SERENITY" PAGE X'0D0A'
    VERIFY OFFSET 2 DATA '2222'
    '
  , ''
  );



