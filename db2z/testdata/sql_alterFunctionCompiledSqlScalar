ALTER FUNCTION GREER
  ALTER ALL VERSIONS
  SECURED
;

ALTER FUNCTION CHLOE
  VERSION 1
  SECURED
;

ALTER SPECIFIC FUNCTION SCOTT
  REPLACE ACTIVE VERSION
  ()
  RETURNS INT
  SECURED
  RETURN COS(0)
;

ALTER FUNCTION BRODY (
    CHARACTER
    ,CHAR
    ,CHARACTER (1)
    ,CHAR(1)
    ,CHARACTER VARYING (111)
    ,CHAR VARYING (222)
    ,VARCHAR(333)
    ,CHARACTER CCSID ASCII
    ,CHARACTER CCSID EBCDIC
    ,CHARACTER CCSID UNICODE
    ,CHARACTER FOR SBCS DATA
    ,CHARACTER FOR MIXED DATA
    ,CHARACTER FOR BIT DATA
    ,CHAR VARYING (444) CCSID UNICODE FOR BIT DATA
    ,CHARACTER LARGE OBJECT (1M)
    ,CHAR LARGE OBJECT (4K) CCSID EBCDIC
    ,CLOB (1G) FOR MIXED DATA
    ,CHAR LARGE OBJECT (4G) CCSID UNICODE FOR SBCS DATA
    )
    REPLACE VERSION V2
    (A GRAPHIC (1)
    ,B VARGRAPHIC(2) CCSID UNICODE
    ,C DBCLOB(3K) CCSID EBCDIC
    )
    RETURNS TIMESTAMP (17) WITHOUT TIME ZONE
    RETURN CURRENT TIMESTAMP;
  ;

ALTER FUNCTION JOHANSEN (
    BINARY
    , BINARY VARYING (2)
    , VARBINARY (3)
    , BINARY LARGE OBJECT
    , BLOB
    )
  ADD VERSION V4
    (
      A DATE
    , B TIME
    , C TIMESTAMP
    , D TIMESTAMP WITH TIME ZONE
    , E ROWID
    , F XML
    )
    RETURNS INT
    RETURN SELECT COUNT(*) FROM SYSIBM.SYSDUMMY1
;

ALTER FUNCTION VOLKER
  ACTIVATE VERSION 314159
;

