CREATE VARIABLE X INTEGER;
CREATE VARIABLE X INTEGER DEFAULT NULL;
CREATE VARIABLE X INTEGER DEFAULT 7;
CREATE VARIABLE X TIMESTAMP WITH TIME ZONE DEFAULT CURRENT_TIMESTAMP;
CREATE VARIABLE X TIMESTAMP WITH TIMEZONE DEFAULT CURRENT_TIMESTAMP;
CREATE VARIABLE A.X INTEGER;
CREATE VARIABLE A.X INTEGER DEFAULT NULL;
CREATE VARIABLE A.X INTEGER DEFAULT 7;
CREATE VARIABLE A.X TIMESTAMP WITH TIME ZONE DEFAULT CURRENT_TIMESTAMP;
CREATE VARIABLE A.X TIMESTAMP WITH TIMEZONE DEFAULT CURRENT_TIMESTAMP;
CREATE VARIABLE A.X PUBLIC.GENERAL;

