CALL SYSPROC.DSNUTILV(
  'DBB.STUFF'
  , 'NO'
  , 'CHECK LOB TABLESPACE DB001.TS0001
      EXCEPTIONS 3 SORTDEVT SYSDA
      SORTNUM 4
    '
  , ''
  );

CALL SYSPROC.DSNUTILV(
  'DBB.STUFF'
  , 'NO'
  , '
  CHECK LOB TABLESPACE
  BBC.ONE
  SHRLEVEL CHANGE
  EXCEPTIONS 5
    '
  , ''
  );

