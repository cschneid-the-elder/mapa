ALTER PROCEDURE DESTINY
  REPLACE VERSION VGhlIFNlY3JldA  --==
  ASUTIME NO LIMIT
  IF YEAR(CURRENT DATE) > 2000 THEN
    INSERT INTO T1
      ( C1
      , C2
      , C3
      )
      VALUES 
      ( CURRENT DATE
      , 42
      , 'TWELVE'
      )
    ;
  END IF;
@


ALTER PROCEDURE DESTINY
  REPLACE ACTIVE VERSION
ALTER PROCEDURE TACHI.ROCINANTE.AMOS
  EXTERNAL NAME IEFBR14
  LANGUAGE ASSEMBLE
  PARAMETER STYLE SIMPLE CALL
  NOT DETERMINISTIC
  PACKAGE PATH 'BALTIMORE'
  MODIFIES SQL DATA
  DBINFO
;
@

ALTER PROCEDURE DESTINY.YOUNG
ADD VERSION 2.0
     SELECT MAX(BONUS), MIN(BONUS), AVG(BONUS)
       INTO :WS-MAX, :WS-MIN, :WS-AVG
       FROM FARSCAPE.STUFF
       WITH UR
       QUERYNO 23
;
@

ALTER PROCEDURE DESTINY.YOUNG
ACTIVATE VERSION 3.14159
@

ALTER PROCEDURE DESTINY.RUSH
REGENERATE ACTIVE VERSION
@

ALTER PROCEDURE DESTINY.WRAY
REGENERATE VERSION 3.14159265358979
@

ALTER PROCEDURE DESTINY.TELFORD
REGENERATE ACTIVE VERSION
USING APPLICATION COMPATIBILITY V13R00
@

ALTER PROCEDURE DESTINY.SCOTT
REGENERATE VERSION 3.0
USING APPLICATION COMPATIBILITY V13R01M01
@

ALTER PROCEDURE DESTINY.SCOTT
REGENERATE VERSION 3.0
USING APPLCOMPAT V13R01M01
@

ALTER PROCEDURE DESTINY.KIVA
DROP VERSION 1.0
@

ALTER PROCEDURE UPDATE_1
      ALTER ACTIVE VERSION
      NOT DETERMINISTIC
      CALLED ON NULL INPUT
      ALLOW DEBUG MODE
      ASUTIME LIMIT 10
@

ALTER PROCEDURE UPDATE_1
      REPLACE VERSION V2 (P1 INTEGER, P2 CHAR(5))
      MODIFIES SQL DATA
      UPDATE EMP SET C1 = C1 * C2
            WHERE K1 = K2;
@

ALTER PROCEDURE UPDATE_1
      ADD VERSION V3 (P1 INTEGER, P2 CHAR(5))
        UPDATE EMP SET C1 = C1 * (C2*10)
            WHERE K1 = K2;
@


