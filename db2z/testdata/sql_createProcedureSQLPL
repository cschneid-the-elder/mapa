
--#SET TERMINATOR @

CREATE PROCEDURE TACHI.ROCINANTE.HOLDEN
  (IN numr INTEGER,
   IN deno INTEGER,
   OUT rslt INTEGER)
  DYNAMIC RESULT SETS 4
  BEGIN
   DECLARE ovfl CONDITION for SQLSTATE '22003';
   DECLARE CONTINUE HANDLER FOR ovfl
     RESIGNAL SQLSTATE '22375';
   IF deno = 0 THEN
     SIGNAL ovfl;
   ELSE
     SET rslt = numr / deno;
   END IF;
  END
;
@

CREATE PROCEDURE TACHI.ROCINANTE.HOLDEN
  RESULT SETS 15
  UPDATE T1 SET C1 = CURRENT DATE WHERE K1 = CURRENT TIMESTAMP
;
@

CREATE PROCEDURE TACHI.ROCINANTE.HOLDEN
  DYNAMIC RESULT SET 8
  WRAPPED
  aeiouandsometimesY_asdf0qwerty:redorange3yellow
  greenblue_indigo:violet7theexpanse

@

CREATE PROCEDURE TACHI.ROCINANTE.HOLDEN
  DYNAMIC RESULT SETS 15
  ALTER TABLE T1
    FOREIGN KEY R1 (C1) REFERENCES T2(C1)
;

@

CREATE PROCEDURE TACHI.ROCINANTE.HOLDEN
  DYNAMIC RESULT SETS 16
CREATE TABLE TACHI.ROCINANTE
    ( HOLDEN CHARACTER(8) NOT NULL
          REFERENCES CERES (MILLER PERIOD BUSINESS_TIME, DAWES PERIOD BUSINESS_TIME) ON DELETE SET NULL
          NOT ENFORCED ENABLE QUERY OPTIMIZATION
    , NAOMI  CHAR VARYING (255) WITH DEFAULT SESSION_USER IMPLICITLY HIDDEN
    , AMOS   BINARY VARYING (255)
    , ALEX   INTEGER GENERATED AS IDENTITY (
             START WITH 27
             INCREMENT BY 18
             MINVALUE 28
             MAXVALUE 182
             CYCLE
             CACHE 7
             ORDER)
    , AVASARALA GENERATED ALWAYS FOR EACH ROW ON UPDATE AS ROW CHANGE TIMESTAMP
    , BOBBIE TIMESTAMP NOT NULL WITH DEFAULT
    )
  IN DATABASE THEBELT
  PARTITION BY RANGE (AVASARALA) (PARTITION 1 ENDING AT ('2025-02-28 23:59:59.999999'), PARTITION 2 ENDING AT (MAXVALUE))
  EDITPROC J3141592 WITH ROW ATTRIBUTES
  VALIDPROC J4815162
  AUDIT ALL
  OBID 2342
  DATA CAPTURE CHANGES
  WITH RESTRICT ON DROP
  CCSID UNICODE
  VOLATILE CARDINALITY
  LOGGED
  COMPRESS YES HUFFMAN
  APPEND YES
  DSSIZE 2G
  BUFFERPOOL SOL
  MEMBER CLUSTER
  TRACKMOD YES
  PAGENUM RELATIVE
  NO KEY LABEL
;
@

CREATE PROCEDURE ROCINANTE.HOLDEN
  DYNAMIC RESULT SETS 23
  INSERT INTO T1 (C1,C2,C3) SELECT C1,C2,C3 FROM T2 WHERE K1 = CURRENT TIMESTAMP;
@

CREATE PROCEDURE HOLDEN
  DYNAMIC RESULT SET 42
  SELECT C1,C2,C3,C4 INTO V1,V2,V3,V4 FROM T1
  ;
@

