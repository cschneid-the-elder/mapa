EXEC SQL 
     DECLARE CURSOR001 CURSOR FOR
     SELECT * FROM ATUIN.FOURECKS
     OPTIMIZE FOR 42 ROWS
     FOR READ ONLY
END-EXEC

EXEC SQL
     DECLARE CURSOR001 CURSOR FOR
     SELECT * FROM ATUIN.FOURECKS
     FETCH FIRST 42 ROWS ONLY
END-EXEC

EXEC SQL
     DECLARE CURSOR001 CURSOR FOR
     SELECT ROWID
       FROM FINAL TABLE 
         (INSERT INTO FARSCAPE.EMP_PHOTO_RESUME
           (NB)
           SELECT NB FROM FARSCAPE.EMP)
     FETCH FIRST 15 ROWS ONLY
END-EXEC

EXEC SQL
      DECLARE CURSOR001 CURSOR FOR
      SELECT NB, NAME, MANAGER
      FROM FARSCAPE.DEPT
      WHERE ADM_DEPT = 'A00'
END-EXEC

EXEC SQL
      DECLARE CURSOR001 CURSOR FOR
      SELECT NB, NAME, MANAGER
      FROM FARSCAPE.DEPT
      WHERE ADM_DEPT = 'A00'
      FOR READ ONLY WITH RS USE AND KEEP EXCLUSIVE LOCKS
END-EXEC

EXEC SQL 
     DECLARE CURSOR002 CURSOR FOR STATEMENT16
END-EXEC

EXEC SQL
     DECLARE CURSOR003 CURSOR WITH HOLD FOR
     SELECT * FROM FARSCAPE.EMP
       FOR UPDATE OF WORKDEPT, PHONENO, JOB, EDLEVEL, SALARY
END-EXEC

EXEC SQL
     DECLARE CURSOR004 CURSOR WITH HOLD WITH RETURN FOR
     SELECT NB, NAME
     FROM FARSCAPE.PROJECT
     WHERE DEPT_NB = 'AAA'
END-EXEC

EXEC SQL
     DECLARE CURSOR005 SENSITIVE STATIC SCROLL CURSOR FOR
     SELECT NB, NAME, MANAGER
     FROM FARSCAPE.DEPT
     WHERE ADM_DEPT = '000'
END-EXEC

EXEC SQL
     DECLARE CURSOR006 INSENSITIVE SCROLL CURSOR FOR
     SELECT NB, NAME, MANAGER
     FROM FARSCAPE.DEPT
     WHERE DEPT_NB = 'B12'
END-EXEC

EXEC SQL
     DECLARE TURTLE
     SENSITIVE DYNAMIC SCROLL CURSOR FOR
     SELECT NB, CUSTOMER_NAME, AMT, DATE FROM ORDER 
     WHERE AMT > 1000
     FOR UPDATE OF SNIDE_NOTES
END-EXEC

EXEC SQL
     DECLARE CURSOR001 CURSOR 
     WITH ROWSET POSITIONING FOR MYCURSOR
END-EXEC

EXEC SQL
     DECLARE CURSOR-001 CURSOR FOR
     SELECT VIMES, NOBBY FROM ATUIN.FOURECKS
END-EXEC

EXEC SQL
     DECLARE CSR-ABC CURSOR FOR
     SELECT VIMES, NOBBY FROM ATUIN.FOURECKS
END-EXEC


