,'JOHN'
,'T'
,'CRICHTON'
,'F01'
,'3141'
,'1981-08-07'
,'PILOT'
,16
,'N'
,'1961-05-05'
,16384
,512
,2048);

INSERT INTO ZED.STUFF
     SELECT *
     FROM FARSCAPE.STUFF;

INSERT INTO SESSION.STUFF
     SELECT *
     FROM FARSCAPE.STUFF
     WHERE DEPT='F01';

INSERT INTO FARSCAPE.THINGS_AND_STUFF(NB, ROWID)
     VALUES (:WS_NB, DEFAULT);

INSERT INTO B.THINGS_AND_STUFF
     OVERRIDING USER VALUE
     SELECT * FROM FARSCAPE.THINGS_AND_STUFF;

EXEC SQL
     INSERT INTO T1
     VALUES (:WS_VAL1:WS_VAL1_IND)
     FOR :WS_NB_ROWS ROWS
     ATOMIC
END-EXEC

EXEC SQL
     INSERT INTO T2
     (C1, C2) 
     VALUES
     (:WS_VAL1:WS_VAL1_IND, :WS_VAL2:WS_VAL2_IND)
     FOR 10 ROWS 
     NOT ATOMIC CONTINUE ON SQLEXCEPTION
END-EXEC 

EXEC SQL
     INSERT INTO T2
     (C1, C2)
     VALUES
     (:WS_VAL1:WS_VAL1_IND, :WS_VAL2:WS_VAL2_IND)
     FOR 8 ROWS 
     NOT ATOMIC CONTINUE ON SQLEXCEPTION
END-EXEC

EXEC SQL
     INSERT INTO ARCONIA
     (NAME, FLOOR)
     VALUES
     ('CHARLES', 7),
     ('OLIVER', 8),
     ('MABEL', 9)
END-EXEC

EXEC SQL
     INSERT INTO ARCONIA
     (NAME)
     VALUES
     'CHARLES', 
     'OLIVER', 
     'MABEL' 
END-EXEC


