SET ENCRYPTION PASSWORD :HV1;
SET ENCRYPTION PASSWORD 'pa55w0rd';
SET ENCRYPTION PASSWORD :HV1 WITH HINT :HV2;
SET ENCRYPTION PASSWORD :HV1 WITH HINT 'h1n+';
SET ENCRYPTION PASSWORD 'pa55w0rd' WITH HINT :HV2;
SET ENCRYPTION PASSWORD 'pa55w0rd' WITH HINT 'h1n+';

SET ENCRYPTION PASSWORD = :HV1;
SET ENCRYPTION PASSWORD = 'pa55w0rd';
SET ENCRYPTION PASSWORD = :HV1 WITH HINT :HV2;
SET ENCRYPTION PASSWORD = :HV1 WITH HINT 'h1n+';
SET ENCRYPTION PASSWORD = 'pa55w0rd' WITH HINT :HV2;
SET ENCRYPTION PASSWORD = 'pa55w0rd' WITH HINT 'h1n+';

SET ENCRYPTION PASSWORD :HV1;
SET ENCRYPTION PASSWORD 'pa55w0rd';
SET ENCRYPTION PASSWORD :HV1 WITH HINT = :HV2;
SET ENCRYPTION PASSWORD :HV1 WITH HINT = 'h1n+';
SET ENCRYPTION PASSWORD 'pa55w0rd' WITH HINT = :HV2;
SET ENCRYPTION PASSWORD 'pa55w0rd' WITH HINT = 'h1n+';

SET ENCRYPTION PASSWORD = :HV1;
SET ENCRYPTION PASSWORD = 'pa55w0rd';
SET ENCRYPTION PASSWORD = :HV1 WITH HINT = :HV2;
SET ENCRYPTION PASSWORD = :HV1 WITH HINT = 'h1n+';
SET ENCRYPTION PASSWORD = 'pa55w0rd' WITH HINT = :HV2;
SET ENCRYPTION PASSWORD = 'pa55w0rd' WITH HINT = 'h1n+';

SET ENCRYPTION 
    PASSWORD :HV1;

