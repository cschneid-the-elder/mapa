CALL SYSPROC.DSNUTILV(
  'DBB.STUFF'
  , 'NO'
  , 'CHECK DATA TABLESPACE 'ABC'.'DEF' INCLUDE XML TABLESPACES(
     TABLE KERBEROUS.PROMETHEUS XMLCOLUMN EYK
     ,TABLESPACE "KERBEROUS"."PROMETHEUS"
     )
  '
  , ''
  );

CALL SYSPROC.DSNUTILV(
  'DBB.STUFF'
  , 'NO'
  , 'CHECK DATA TABLESPACE 'ABC'.'DEF' INCLUDE XML TABLESPACES(
     TABLE "KERBEROUS"."PROMETHEUS" XMLCOLUMN "EYK"
     ,TABLESPACE "KERBEROUS"."PROMETHEUS"
     ,TABLE "KERBEROUS"."PROMETHEUS" XMLCOLUMN "MAURA"
     ,TABLESPACE "KERBEROUS"."PROMETHEUS"
     )
  '
  , ''
  );

CALL SYSPROC.DSNUTILV(
  'DBB.STUFF'
  , 'NO'
  , 'CHECK DATA TABLESPACE 'ABC'.'DEF' INCLUDE XML TABLESPACES(
     TABLE "KERBEROUS"."PROMETHEUS" XMLCOLUMN "EYK"
     ,TABLESPACE "KERBEROUS"."PROMETHEUS"
     ,TABLE "KERBEROUS"."PROMETHEUS" XMLCOLUMN "MAURA"
     ,TABLESPACE "KERBEROUS"."PROMETHEUS"
     ) 
     CLONE 
     SHRLEVEL REFERENCE 
     SHRLEVEL CHANGE
     DRAIN_WAIT 42
     RETRY 18
     RETRY_DELAY 37
     SCOPE PENDING
     SCOPE AUXONLY
     SCOPE ALL
     SCOPE REFONLY
     SCOPE XMLSCHEMAONLY
     AUXERROR REPORT
     AUXERROR INVALIDATE
     LOBERROR REPORT
     LOBERROR INVALIDATE
     XMLERROR REPORT
     XMLERROR INVALIDATE
     FOR EXCEPTION 
         IN KERBEROUS.PROMETHEUS USE THEBELT.TACHI
         IN "KERBEROUS".PROMETHEUS USE THEBELT.TACHI
         IN KERBEROUS."PROMETHEUS" USE THEBELT.TACHI
         IN "KERBEROUS"."PROMETHEUS" USE THEBELT.TACHI
         IN KERBEROUS.PROMETHEUS USE "THEBELT".TACHI
         IN KERBEROUS.PROMETHEUS USE THEBELT."TACHI"
         IN KERBEROUS.PROMETHEUS USE "THEBELT"."TACHI"
         IN 'KERBEROUS'.PROMETHEUS USE THEBELT.TACHI
         IN KERBEROUS.'PROMETHEUS' USE THEBELT.TACHI
         IN 'KERBEROUS'.'PROMETHEUS' USE THEBELT.TACHI
         IN KERBEROUS.PROMETHEUS USE 'THEBELT'.TACHI
         IN KERBEROUS.PROMETHEUS USE THEBELT.'TACHI'
         IN KERBEROUS.PROMETHEUS USE 'THEBELT'.'TACHI'
     DELETE NO
     DELETE YES
     DELETE YES LOG YES
     DELETE YES LOG NO
     EXCEPTIONS 99
     ERRDDN SYSERR
     WORKDDN SYSUT1
     WORKDDN SYSUT1, SORTOUT
     WORKDDN , SYSUT2
     PUNCHDDN SYSPUNCH
     SORTDEVT WRK1
     SORTNUM 7
  '
  , ''
  );

CALL SYSPROC.DSNUTILV(
  'DBB.STUFF'
  , 'NO'
  , 'CHECK DATA TABLESPACE 'ABC'.DEF PART 1'
  , ''
  );

CALL SYSPROC.DSNUTILV(
  'DBB.STUFF'
  , 'NO'
  , 'CHECK DATA TABLESPACE 'ABC'.DEF PART 1 CLONE'
  , ''
  );

CALL SYSPROC.DSNUTILV(
  'DBB.STUFF'
  , 'NO'
  , 'CHECK DATA TABLESPACE 'ABC'.DEF CLONE'
  , ''
  );

CALL SYSPROC.DSNUTILV(
  'DBB.STUFF'
  , 'NO'
  , 'CHECK DATA TABLESPACE 'ABC'.'DEF' INCLUDE XML TABLESPACES(
     TABLE "KERBEROUS"."PROMETHEUS" XMLCOLUMN "EYK"
     ,TABLESPACE "KERBEROUS"."PROMETHEUS"
     ,TABLE "KERBEROUS"."PROMETHEUS" XMLCOLUMN "MAURA"
     ,TABLESPACE "KERBEROUS"."PROMETHEUS"
     ) 
     CLONE 
     SHRLEVEL REFERENCE 
     SHRLEVEL CHANGE
     DRAIN_WAIT 42
     RETRY 18
     RETRY_DELAY 37
     SCOPE (PENDING)
     SCOPE (AUXONLY)
     SCOPE (ALL)
     SCOPE (REFONLY)
     SCOPE (XMLSCHEMAONLY)
     AUXERROR REPORT
     AUXERROR INVALIDATE
     LOBERROR REPORT
     LOBERROR INVALIDATE
     XMLERROR REPORT
     XMLERROR INVALIDATE
     FOR EXCEPTION 
         IN KERBEROUS.PROMETHEUS USE THEBELT.TACHI
         IN "KERBEROUS".PROMETHEUS USE THEBELT.TACHI
         IN KERBEROUS."PROMETHEUS" USE THEBELT.TACHI
         IN "KERBEROUS"."PROMETHEUS" USE THEBELT.TACHI
         IN KERBEROUS.PROMETHEUS USE "THEBELT".TACHI
         IN KERBEROUS.PROMETHEUS USE THEBELT."TACHI"
         IN KERBEROUS.PROMETHEUS USE "THEBELT"."TACHI"
         IN 'KERBEROUS'.PROMETHEUS USE THEBELT.TACHI
         IN KERBEROUS.'PROMETHEUS' USE THEBELT.TACHI
         IN 'KERBEROUS'.'PROMETHEUS' USE THEBELT.TACHI
         IN KERBEROUS.PROMETHEUS USE 'THEBELT'.TACHI
         IN KERBEROUS.PROMETHEUS USE THEBELT.'TACHI'
         IN KERBEROUS.PROMETHEUS USE 'THEBELT'.'TACHI'
     DELETE NO
     DELETE YES
     DELETE YES LOG YES
     DELETE YES LOG NO
     EXCEPTIONS 99
     ERRDDN (SYSERR)
     WORKDDN (SYSUT1)
     WORKDDN (SYSUT1, SORTOUT)
     WORKDDN (, SYSUT2)
     PUNCHDDN (SYSPUNCH)
     SORTDEVT (WRK1)
     SORTNUM 7
  '
  , ''
  );


