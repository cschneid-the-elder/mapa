EXEC DLI ISRT
     SEGMENT(EDWIN) WHERE (PAINE=PAINE1)
     SEGMENT(CHARLES) FROM(ROWLAND)
     SEGMENT(CRYSTAL) FROM(PALACE)
END-EXEC

EXEC DLI INSERT USING PCB(DBDA)
     VARIABLE
     FIRST
     LAST
     CURRENT
     SEGMENT(CASE)
     SEGMENT((WS-CASE))
     SEGLENGTH(LENGTH(WS-CASE))
     FROM(WS-CASE-DATA)
     MOVENEXT('1')
     GETFIRST('2')
     SET('3')
     SETCOND('4')
     SETZERO('5')
     WHERE(EDWIN=PAINE AND CHARLES=ROWLAND AND CRYSTAL=PALACE)
     FIELDLENGTH(LENGTH(PAINE),LENGTH(ROWLAND),LENGTH(PALACE))
     KEYS(KY-AREA)
     KEYLENGTH(LENGTH(KY-AREA))
END-EXEC

