EXEC DLI GU
   SEGMENT(EDWIN) WHERE(EDWIN=EDWIN1)
   SEGMENT(CHARLES) WHERE(CHARLES=CHARLES1)
   SEGMENT(CRYSTAL) INTO(AN-AREA) WHERE(DATE=DATE1);

EXEC DLI
     GET UNIQUE
     USING PCB(DBDA)
     KEYFEEDBACK(WS-KYFDBK)
     FEEDBACKLEN(LENGTH(WS-KYFDBK))
     INTO(DATA-AREA)
     VARIABLE
     LAST
     SEGMENT('EDWIN')
     SEGMENT( ( WS-EDWIN ) )
     SEGLENGTH(LENGTH(WS-EDWIN))
     OFFSET(42)
     INTO(EDWIN-AREA)
     LOCKED
     LOCKCLASS('A')
     MOVENEXT('X')
     GETFIRST('1')
     SET('2')
     SETCOND('3')
     SETZERO('4')
     SETPARENT
     WHERE(CRYSTAL>=PALACE AND CHARLES<=ROWLAND)
     FIELDLENGTH(LENGTH(PALACE),LENGTH(ROWLAND))
     KEYS(WS-KY)
     KEYLENGTH(LENGTH(WS-KY))
END-EXEC
