EXEC DLI GNP
     INTO(REC);

EXEC DLI
     GET NEXT IN PARENT
     USING PCB(DBDA)
     KEYFEEDBACK(WS-KYFDBK)
     FEEDBACKLEN(LENGTH(WS-KYFDBK))
     INTO(DATA-AREA)
     VARIABLE
     FIRST
     LAST
     CURRENT
     SEGMENT('EDWIN')
     SEGMENT( ( WS-EDWIN ) )
     SEGLENGTH(LENGTH(WS-EDWIN))
     OFFSET(42)
     INTO(EDWIN-AREA)
     LOCKED
     LOCKCLASS('A')
     MOVENEXT('X')
     GETFIRST('1')
     SET('2')
     SETCOND('3')
     SETZERO('4')
     SETPARENT
     WHERE(CRYSTAL>=PALACE AND CHARLES<=ROWLAND)
     FIELDLENGTH(LENGTH(PALACE),LENGTH(ROWLAND))
     KEYS(WS-KY)
     KEYLENGTH(LENGTH(WS-KY))
END-EXEC
